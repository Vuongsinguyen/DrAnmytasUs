{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "routes": [
    {
      "src": "/wp-content/themes/(.*)/fonts/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/wp-content/themes/$1/fonts/$2"
    },
    {
      "src": "/(.*)\\.woff2",
      "headers": {
        "Content-Type": "font/woff2",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/$1.woff2"
    },
    {
      "src": "/(.*)\\.woff",
      "headers": {
        "Content-Type": "font/woff",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/$1.woff"
    },
    {
      "src": "/(.*)\\.ttf",
      "headers": {
        "Content-Type": "font/ttf",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/$1.ttf"
    },
    {
      "src": "/(.*)\\.eot",
      "headers": {
        "Content-Type": "application/vnd.ms-fontobject",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/$1.eot"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/wp-content/themes/(.*)/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/(.*).woff",
      "headers": [
        {
          "key": "Content-Type",
          "value": "font/woff"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/(.*).woff2",
      "headers": [
        {
          "key": "Content-Type",
          "value": "font/woff2"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/(.*).ttf",
      "headers": [
        {
          "key": "Content-Type",
          "value": "font/ttf"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/(.*).eot",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/vnd.ms-fontobject"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
